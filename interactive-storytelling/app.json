{
  "expo": {
    "name": "Human Stories",
    "slug": "interactive-storytelling",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/koryaslogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "transparent",
      "translucent": true,
      "barStyle": "dark-content"
    },
    "android": {
      "package": "com.koryas.humanstories",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff00"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "eas": {
        "projectId": "8108e644-6e28-4d35-921f-52d21cb4d6cd"
      }
    }
  }
}
